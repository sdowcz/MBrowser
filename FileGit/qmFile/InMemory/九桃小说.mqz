  transition: margin 0.5s ease;\n        display: flex;\n        justify-content: center;\n        align-items: center;\n\t\t  }\n\t    .bt-box:hover .bottom {\n\t\t    margin: 40px 0 0 10px;\n\t\t  }\n\t    .bt-box:hover .top1 {\n\t\t    margin: -120px 0 0 0;\n\t\t    line-height: 35px;\n\t  \t}\n\t    .bt-box:hover .top2 {\n\t\t    margin: -220px 0 0 0;\n\t\t    line-height: 35px;\n\t\t  }\n\t    .bt-box:hover .top3 {\n\t\t    margin: -320px 0 0 0;\n\t\t    line-height: 35px;\n\t\t  }\n\t    .bt-box:hover .top4 {\n\t\t    margin: -420px 0 0 0;\n\t\t    line-height: 35px;\n\t\t  }\n       .bt-box:hover .top5 {\n\t\t    margin: -520px 0 0 0;\n\t\t    line-height: 35px;\n\t\t  }\n\t    .bt-box .xiaoA:hover {\n\t\t\t  color: #fff;\n\t\t\t  background: #2f7fd5;\n\t\t\t  text-decoration: none;\n\t\t  }\t\n\t\t\n\t    #button {\n\t\t    position: fixed;\n\t\t    font-size: 12px;\n\t\t    text-transform: uppercase;\n\t\t    right: 5%;\n        top: 90%;\n\t\t    width: 40px;\n\t\t    height: 40px;\n\t\t    color: white;\n\t\t    cursor: pointer;\n\t\t    animation: pulse 1s infinite alternate;\n        transition: background #62aeff 0.4s, border 0.2s, margin 0.2s;\n      }\n  \n\t    #button:hover {\n\t\t    margin-top: -1px;\n\t\t    animation: none;\n      }\n  \n\t    #button:active {\n\t\t    border-bottom-width: 0;\n\t\t    margin-top: 0px;\n      }\n  \n      @keyframes pulse {\n\t\t    0% {\n\t\t\t    margin-top: 0px;\n\t\t    }\n\t\t    100% {\n\t\t\t    margin-top: 5px; \n\t\t    } \n\t    }\n    \u003c/style\u003e"},{"a":"body","b":"#文本","c":"\u003cdiv id\u003d\"button\"\u003e\n      \u003cdiv class\u003d\"bt-box\"\u003e\n        \u003cdiv class\u003d\"xiaoA bg-1\"\u003e设置\u003c/div\u003e\n        \u003cdiv class\u003d\"top1 bt-box-p\" onclick\u003d\"记忆()\"\u003e记忆\u003c/div\u003e\n        \u003cdiv class\u003d\"top2 bt-box-p\" onclick\u003d\"ds()\"\u003e大小\u003c/div\u003e\n        \u003cdiv class\u003d\"top3 bt-box-p\"  onclick\u003d\"hj()\"\u003e行距\u003c/div\u003e\n        \u003cdiv class\u003d\"top4 bt-box-p\" onclick\u003d\"bg()\"\u003e背景\u003c/div\u003e\n        \u003cdiv class\u003d\"top5 bt-box-p\" onclick\u003d\"ys()\"\u003e颜色\u003c/div\u003e\n      \u003c/div\u003e\n    \u003c/div\u003e\n\u003cscript\u003e\nfunction setCookie(cname,cvalue,exdays){\n\tvar d \u003d new Date();\n\td.setTime(d.getTime()+(exdays*24*60*60*1000));\n\tvar expires \u003d \"expires\u003d\"+d.toGMTString();\n\tdocument.cookie \u003d cname+\"\u003d\"+cvalue+\"; \"+expires;\n}\nfunction getCookie(cname){\n\tvar name \u003d cname + \"\u003d\";\n\tvar ca \u003d document.cookie.split(\u0027;\u0027);\n\tfor(var i\u003d0; i\u003cca.length; i++) {\n\t\tvar c \u003d ca[i].trim();\n\t\tif (c.indexOf(name)\u003d\u003d0) { return c.substring(name.length,c.length); }\n\t}\n\treturn \"\";\n}\nfunction ds(){\nvar ds\u003dprompt(\"请输入对应数值\");\nsetCookie(\"字体大小\",ds,30);\n}\nfunction ys(){\nvar ys\u003dprompt(\"请输入对应数值\");\nsetCookie(\"字体颜色\",ys,30);\n}\nfunction hj(){\nvar hj\u003dprompt(\"请输入对应数值\");\nsetCookie(\"行距\",hj,30);\n}\nfunction bg(){\nvar bg\u003dprompt(\"请输入对应数值\");\nsetCookie(\"背景\",bg,30);\n}\nfunction 记忆(){\nvar size\u003dgetCookie(\"字体大小\");\ndocument.getElementById(\u0027mtext\u0027).style.fontSize\u003dsize+\u0027px\u0027;\nvar color\u003dgetCookie(\"字体颜色\");\ndocument.getElementById(\u0027mtext\u0027).style.color\u003dcolor;\nvar height\u003dgetCookie(\"行距\");\ndocument.getElementById(\u0027mtext\u0027).style.lineHeight\u003dheight+\u0027px\u0027;\nvar bg\u003dgetCookie(\"背景\");\ndocument.body.style.backgroundColor\u003dbg;\n}\n\u003c/script\u003e"}],"version":20210928}